# M1 Data Ingestion Service - Environment Variables
# Copy this file to .env and fill in the values

# GCP Configuration
PROJECT_ID=gg-poker
DATASET=prod
TABLE=hand_summary
REGION=us-central1

# Environment
ENVIRONMENT=development  # development, staging, production

# Flask Configuration
PORT=8001
DEBUG=false

# Dataflow Configuration
DATAFLOW_RUNNER=DataflowRunner  # DataflowRunner or DirectRunner (local)
MAX_WORKERS=10

# Google Cloud credentials (local development only)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
